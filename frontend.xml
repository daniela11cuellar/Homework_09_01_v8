<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name = "UI Testing">
    <listeners>
        <listener class-name="frontend.utils.ListenerTest"/>
    </listeners>
    <test name = "Regression Automation Practice">
        <parameter name="browser" value="${browser}"/>
        <parameter name="url" value="https://rahulshettyacademy.com/AutomationPractice/"/>
        <classes>
            <class name = "frontend.tests.TestCasesAutomationPractice" />
        </classes>
    </test>
    <test name = "Regression Demo QA">
        <parameter name="browser" value="${browser}"/>
        <parameter name="url" value="https://demoqa.com/"/>
        <classes>
            <class name = "frontend.tests.TestCasesDemo" />
        </classes>
    </test>
    <test name = "Regression Library">
        <parameter name="browser" value="${browser}"/>
        <parameter name="url" value="https://www.loc.gov/"/>
        <classes>
            <class name = "frontend.tests.TestCasesLibrary" />
        </classes>
    </test>
</suite>
